  "devDependencies": {
    "@types/jest": "^27.4.1",
    "esbuild": "^0.14.25",
    "jest": "^27.5.1",
    "npm-dts": "^1.3.11",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.2"
  }
